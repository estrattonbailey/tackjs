    "tack",
    "positioning"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/estrattonbailey/tackjs/issues"
  },
  "homepage": "https://github.com/estrattonbailey/tackjs#readme",
  "devDependencies": {
    "ava": "^2.1.0",
    "microbundle": "^0.11.0",
    "ts-node": "^8.6.0"
  },
  "ava": {
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  }

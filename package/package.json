{
  "name": "tackjs",
  "version": "0.0.1",
  "description": "A simple toolkit to pin an element relative to another element.",
  "main": "dist/index.js",
  "license": "MIT",
  "repository": "https://github.com/estrattonbailey/tackjs",
  "author": "estrattonbailey",
  "scripts": {
    "lint": "standard 'index.js'",
    "prebuild": "npm run lint",
    "build": "NODE_ENV=production babel index.js -d dist/",
    "watch": "NODE_ENV=development babel -w --no-comments index.js -d dist/",
    "prepublish": "npm run build"
  },
  "keywords": [
  ],
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.21.0",
    "babel-preset-babili": "0.0.10",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "standard": "^8.6.0"
  }
}
